generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model NoteDetail {
  id           String   @id @default(uuid())
  note         String   @db.Text
  createdate   DateTime @default(now())
  scheduledate DateTime
  updateddate  DateTime @updatedAt
}
